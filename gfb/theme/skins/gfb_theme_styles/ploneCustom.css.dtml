/* @group Globalnav */

#portal-globalnav {
    padding: 0;
    height: 32px;
    background: #424951 url(bg_topnavi_break.gif) repeat-x 200px;
    font: 90% Arial, "Helvetica Neue", Helvetica, Geneva, sans-serif;
}

#portal-globalnav li {
    display: block;
    margin: 0;
    padding: 0 24px 0 12px;
    border-right: 1px solid #424951;
    float: left;
    background: #424951 url(bg_topnavi_break.gif) repeat-x
}

#portal-globalnav li a {
    line-height: 32px;
    color: white;
    font-size: 12px;
}

#portal-globalnav li:hover,
#portal-globalnav li.selected {
    background: #A8126A url(global_li_sel.jpg);
}

#portal-globalnav li a:hover {
    color: white;
}

/* @end */

/* @group Navigation */

/*.navTreeLevel1 li,
.navTreeLevel2 li,
.navTreeLevel3 li,
.navTreeLevel4 li {
    background-color: #ffffff;
}*/


.portletNavigationTree, #portal-globalnav ul.submenu {
    font-weight: normal;
    font-style: normal;
    font-size: 90%;
    margin: 0;
}

.portletNavigationTree .portletItem {
    margin: 0;
}
/* dropdown navi background */
#portal-globalnav ul.submenu li {
    background: #e9eaef;
}
.portletNavigationTree li, #portal-globalnav ul.submenu li {
    line-height: 21px;
}

/* hovering for navigation items */
.portletNavigationTree li a:hover, 
#portal-globalnav ul.submenu li.navTreeItem:hover {
    background-color: #d8dbe0;
}

#portal-globalnav ul.submenu li.navTreeItem div:hover {
    background-color: #d8dbe0;
}
/* this one is needed to overwrite the one from dropdownmenu.css */
#portal-globalnav li.selected ul.submenu a:hover {
    background-color: #d8dbe0;
}

.portletNavigationTree li a, 
#portal-globalnav ul.submenu li a {
    color: #000000;
    line-height: 1.6em;
}

li.navTreeItem {
    margin: 1px 0px 1px 0px;
    /*border-top: 1px solid #4a5b6d;
    border-collapse: collapse;*/
    /*background:#e9eaef none repeat scroll 0 0;*/
}

li.navTreeItem div {
    background:#e9eaef none repeat scroll 0 0;
}
#portal-globalnav ul.submenu li.navTreeItem {
    margin: 0;
    border-top: 1px solid #4a5b6d;
    border-collapse: collapse;
}


/* Selected item in navtree level 0 */
.navTreeLevel0 li.navTreeCurrentNode > div {
    background-color: #d8dbe0;
}



.navTreeLevel0 a.navTreeCurrentItem {
    background: url(ico_link_subnavi_active_right.gif) no-repeat scroll 6px 6px;
}



.navTreeLevel0 a.navTreeItemInPath {
    background: url(ico_link_subnavi_active.gif) no-repeat scroll 6px 4px;
}

.navTreeLevel1 a.navTreeCurrentItem {
  background:transparent url(ico_link_subnavi_active_right.gif) no-repeat scroll 16px 6px;
  background-color: #D8DBE0;
}
.navTreeLevel1 a.navTreeItemInPath  {
    background: url(ico_link_subnavi_active.gif) no-repeat scroll 16px 6px;
}


.navTreeLevel2 a.navTreeCurrentItem {
  background:transparent url(ico_link_subnavi_active_right.gif) no-repeat scroll 26px 6px;
  background-color: #D8DBE0;
}
.navTreeLevel2 a.navTreeItemInPath  {
    background: url(ico_link_subnavi_active.gif) no-repeat scroll 26px 6px;
}

.navTreeLevel3 a.navTreeCurrentItem {
  background:transparent url(ico_link_subnavi_active_right.gif) no-repeat scroll 36px 6px;
  background-color: #D8DBE0;
}
.navTreeLevel3 a.navTreeItemInPath  {
    background: url(ico_link_subnavi_active.gif) no-repeat scroll 36px 6px;
}


ul.navTreeLevel0, 
ul.navTreeLevel1, 
ul.navTreeLevel2,
ul.navTreeLevel3 {
  /*background:#e9eaef none repeat scroll 0 0;*/
}

ul.navTreeLevel0 {
  margin:0 1px 0 0 ;
}


/* indent for Level 1 */
.navTreeLevel0 li div a  {
    padding-left: 20px;
    display: block;
    padding-bottom:2px;
    padding-top:2px;
}

/* indent 2 times for Level 3 */
.navTreeLevel1 li div a {
    padding-left: 30px;
    display: block;
}
/* indent 3 times for Level 3 */
.navTreeLevel2 li div a {
    padding-left: 40px;
    display: block;
}
/* indent 4 times for Level 4 */
.navTreeLevel3 li div a {
    padding-left: 50px;
    display: block;
}


/* in the dropdown navi, we need no indenting */
#portal-globalnav ul.submenu li div a {
    padding-left: 0px;
}
#portal-globalnav ul.submenu li {
    padding-left: 0;
    padding-right: 0;
}
#portal-globalnav ul.submenu li div {
    padding-left: 0.5em;
}
/* No item imgs */
#portal-globalnav ul.submenu li div a img{
    display:none;
}


ul.navTree span {
  line-height: 15px;
  font-family: "arial";
}



/* @end */


/* @path_bar */
