table {
    border-collapse: collapse;
    font-size: 1em;
    }


#risktable {
    font-family: Arial, "Helvetica Neue", Helvetica, Geneva, sans-serif;
    font-size: 11px;
    vertical-align: top;
    margin-bottom: 2px;
    display: block;
}


.riskfield {
    padding: 10px 10px;
    background-color: #E7E9EB;
    margin: 1px 0 0 0 ;
}

.risklabel {
  float:left;
  margin-top:0.5em;
  width: 20%;
}
.riskvalue {
    float: left;
}

.strong {
    font-weight: bold;
}

#content td ul li,
#content td ul  {
    padding: 0;
    margin-top: 0px;
}

em {
    font-style: normal;
    font-weight: bold;
}

strong {
    font-weight: bold;
}

blockquote {
    margin: 0.7em 0;
    padding: 0.5em 11px;
}

#region-content div.plain blockquote {
    clear: both;
}

code, kbd {
    background: #333;
    color: #ccc;
    font-family: "Lucida console", Lucida, Monaco, "Courier New", Courier, monospace;
}

pre,
.standalone,
.comment {
    margin: 1em 0em;
    padding: 1em;
    background: #E5E6E8;
    border: 0 none;
    color: #3d3d3d;
    font-family: Arial, "Helvetica Neue", Helvetica, Geneva, sans-serif;
}

.standalone {
    color: #A8126A;
    font-weight: bold;
    background: #E5E6E8 url(ico_link_internal.gif) no-repeat 10px center;
    padding-left: 24px;
}

del {
    color: #999;
    text-decoration: overline;
}

img {
    border: none;
}

.newsImageContainer {
    float: right;
    width: 225px;
}

#parent-fieldname-imageCaption {
    float: right;
    padding-left: 23px;
    font: bold 11px/13px Arial, "Helvetica Neue", Helvetica, Geneva, sans-serif;
}

img.newsImage,
img.image-right,
img.tileImage {
    float: right;
    margin: 8px 0 10px 20px;
}

/* @end */

/* @group forms */

.floated_control {
    float:right; 
    width:20em; 
    margin-top: -1.1em; 
    text-align:right; 
    padding-right:2em;
}
.floated_control select {
    font-size: 90%;
}
a.pluss {
	padding-left: 20px;
	background: url(plus_icon.gif) no-repeat 0 3px;
	margin-left: 20px;
}


div#searchfields,
div.search_medium {
    background-color: #E5E6E8;
    border: 0 none !important;
    padding:  13px 0 0 0;
    min-height: 140px;
    
}
div.search_medium {
    font-family: Arial,sans-serif;
}
div#region-content div.search_medium ul  {
    list-style-type:bullet;
    list-style-image : none;
}
div#region-content li {
    margin-left: 2.5em;
}

.info_of_search {
	width: 549px;
	float:left;
	padding-top:20px;

}

div.search_medium li {
	color: grey;
	padding-bottom: 0px;
	font-size: 95%;
	line-height: 100%;
}
div.search_medium ul {
	margin-left: 1em;

}
.search_results {
	border-bottom: 2px solid #ecedf2;
	clear: both;
}

.searchResults .highlighted { background: #F6F6F6; }

div.search_medium {
	padding: 13px;
	width: 160px;
	float: right;
}
.search_medium h5 {
	color: black;
	margin-bottom: 1em;
}

#searchfields  #SearchableText {
	width: 380px;
	margin-bottom: 10px;
	padding: 2px 6px;
	margin-left:11px;
}

#content label.formQuestion {
    background: #A2A5B4;
    color: white;
    width: 100%;
    padding: 5px 10px;
    margin: 8px 0;
    display: block;
    font: bold 13px/19px Arial, "Helvetica Neue", Helvetica, Geneva, sans-serif;
}

/*body.section-db.template-db_filter h1,
body.section-db.template-db_filter h2,*/

.search-top {
    background: #4B516B;
    color: white;
    padding: 10px 15px;
    margin: 0;
}

#simplesearch ul {
	background: #4B516B;
	height: 25px;
	padding: 0;
	width: 100%;
}
#simplesearch li {
	float: left;
	margin: 0;
	list-style-image: none;
	list-style-type: none;
}

#simplesearch li a {
	color: white;
	font: bold 13px/19px Arial, "Helvetica Neue", Helvetica, Geneva, sans-serif;
	background: transparent url(search_li_arrow.gif) no-repeat 11px 7px;
	float: left;
	margin: 0;
	line-height: 25px;
	padding-left: 28px;
	padding-right: 8px;

}

input.radio_search {
	width: 14px !important;
	margin-right:7px;
	display: block;
	float: left;
}

#searchfields div.column1b ul.search_index {
	margin: 15px 0px 20px 40px;
	line-height: 100%;
}

ul.search_index {
    line-height: 15px;
    list-style-image: url(ico_bullet.gif);
}

div.column1b {
	width: 360px;
	float: left;
    margin: 15px 11px 10px 11px;
}

div.column2b {
    margin: 15px 11px 10px 11px;
	width: 350px;
	float: left;
}  
    

.context {text-transform: none;}

.submit_column {
    margin-top: 0 !important;
    padding-top:0px;
    padding-bottom: 13px !important;
    margin-bottom: 0 !important;
    text-align:right !important;
    height: 1.3em;
    background-color: #a1a8b2;
    border: 1px solid #70767d;
}

#searchfields input.context {
	float: right;
	border: 0;
	margin-right:3.8em;
    margin-top:0.5em;
	font-weight: normal;
	padding-left: 17px;
	background: url(ico_search.gif) no-repeat 0 4px;
}

#searchfields input.context cancelbutton {
	background: url(cancel_button.gif) no-repeat 0 2px;
}

#simplesearch  li a.selected {
	color: black;
	background: #e5e6e8 url(search_li_selected.gif) no-repeat 11px 7px;
}

.info_of_search a {
    font-weight: bold;
	background: url(ico_link_internal.gif) no-repeat 0 4px;
	padding-left: 13px;
}

body.section-db.template-db_filter h2 {
    padding: 5px 8px;
}

/*form {
    #content: 0;
}
*/
body.section-db.template-db_filter #portal-breadcrumbs {
    border-color: white;
}

div#providerinfo,
#providerperson {
    width: 40%;
    float: left;
    padding-left: 7%;
}

#providerinfo label,
div#providerperson label {
    display: block;
    margin-top: 12px;
    padding-bottom: 3px;
    border-bottom: 1px solid #c8cbcc;
    margin-bottom: 4px;
}

#searchfields h5 {
	background-color: #a2a5b4;
	color: white;
	padding: 6px 10px;
	margin: 0 0 17px -10px;
	font-weight: normal;
}

.column2b h5 {
	padding-left: 25px !important;
	margin-left: 0 !important;
	margin-right: -10px !important;
}

.column2b ul {
	margin-left: 30px;
}



/* @end */

/* @group bottom */

#portal-footer {
    float: none;
    line-height: 1.2em;
    margin: 2em 0em 1em 0em;
    padding: 0.5em 0em 1em 0em;
    text-align: center;
}

#portal-footer p {
    margin: 0.25em 0;
}

#portal-footer a {
    text-decoration: none;
    border: none;
}

#portal-footer a:hover {
    text-decoration: underline;
}

#portal-colophon {
    float: none;
    margin: 0 0 1em 0;
    padding: 0 0 1em 0;
    text-align: center;
    color: #666;
}

#portal-colophon ul {
    list-style-image: none;
    list-style-type: none;
}

#portal-colophon ul li {
    display: inline !important;
    font-size: 90%;
    padding: 0 0.75em;
}

#portal-colophon ul li a {
    text-decoration: none;
    border-bottom: 1px #ccc solid;
    color: #666;
}

/* @end */

/* @group syslab */

.ws_address {
    position: relative;
    height: 1%;
    padding: 0;
    margin: 0;
    clear: both;
}

.ws_address_info {
    position: relative;
}

.item_actions {
    position: relative;
    float: right;
    width: 34em;
    padding: 0.5em 0em;
}

.item_actions form {
    display: inline;
    margin: 0;
    padding: 0;
}

.item_actions .actionbtn {
    float: left;
    padding-right: 0.5em;
}

.item_actions .actionbtn a {
    display: block;
    background: transparent url(linkTransparent.gif) 9px 1px no-repeat !important;
    text-decoration: none;
    vertical-align: middle;
    line-height: 1.5em;
    padding: 0.1em 1em 0em 1.5em !important;
    margin-bottom: 1px !important;
}

.item_actions .actionbtn a,
  .item_actions .actionbtn a:link,
  .item_actions .actionbtn a:visited,
  .item_actions .actionbtn a:hover,
  .item_actions .actionbtn a:active {
    color: &dtml-fontColor;;
}

#ras_results dt {
    padding: 0.5em 0 0 0 !important;
    margin: 0;
}

#ras_results dd {
    margin: 0;
    padding: 0.5em 0em !important;
    border-bottom: 1px dotted gray;
}

.fieldRequired {
    background: url(&dtml-portal_url;/required.gif) center left no-repeat;
    padding: 0 0 0 8px;
    color: &dtml-fontColor;

}



#heading_search_results_box {
    background-color:#415063;
    color:whitesmoke;
    font-family:Arial,"Helvetica Neue",Helvetica,Geneva,sans-serif;
    font-size:15px;
    font-size-adjust:none;
    font-stretch:normal;
    font-style:normal;
    font-variant:normal;
    font-weight:bold;
    line-height:20px;
    margin-top:0;
    padding:0;
    vertical-align:middle;
}
#heading_search_results {
    float:left;
    display:block;
    padding: 4px 0.5em 4px 8px;
}    
#search_num_results {
    font-size: 80%;
    vertical-align: baseline;
    font-weight: normal;
    padding-right: 1em;
}

#heading_search_medium {
    float: right;
    width: 160px;
    padding: 4px 13px 4px 12px;
    border-left: 2px solid #3f455d;
}
div.separationBar {
    display:inline;
    border-left: 2px solid #3f455d;
}

.listingBar {
    font-size: 80%;
    font-weight: normal;
    vertical-align: baseline;
    
}
.listingBar a {
    color: whitesmoke;
}

div#link_to_tool {
    margin-top: 1em;
    margin-bottom: 1em;
    
}


